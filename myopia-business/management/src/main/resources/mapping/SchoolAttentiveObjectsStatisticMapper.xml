<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wupol.myopia.business.management.domain.mapper.SchoolAttentiveObjectsStatisticMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wupol.myopia.business.management.domain.model.SchoolAttentiveObjectsStatistic">
    </resultMap>

    <!-- 通用查询条件 -->
    <sql id="Base_Where_Clause">
        <where>
            <trim prefixOverrides="and">
                <if test="@Ognl@isNotEmpty(id)">and `id` = #{id}</if>
                <if test="@Ognl@isNotEmpty(screeningTaskId)">and `screening_task_id` = #{screeningTaskId}</if>
                <if test="@Ognl@isNotEmpty(screeningPlanId)">and `screening_plan_id` = #{screeningPlanId}</if>
                <if test="@Ognl@isNotEmpty(districtId)">and `district_id` = #{districtId}</if>
                <if test="@Ognl@isNotEmpty(visionLabel0Numbers)">and `vision_label_0_numbers` = #{visionLabel0Numbers}</if>
                <if test="@Ognl@isNotEmpty(visionLabel1Numbers)">and `vision_label_1_numbers` = #{visionLabel1Numbers}</if>
                <if test="@Ognl@isNotEmpty(visionLabel2Numbers)">and `vision_label_2_numbers` = #{visionLabel2Numbers}</if>
                <if test="@Ognl@isNotEmpty(visionLabel3Numbers)">and `vision_label_3_numbers` = #{visionLabel3Numbers}</if>
                <if test="@Ognl@isNotEmpty(keyWarningNumbers)">and `key_warning_numbers` = #{keyWarningNumbers}</if>
                <if test="@Ognl@isNotEmpty(createTime)">and `create_time` = #{createTime}</if>
            </trim>
        </where>
    </sql>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, screening_task_id, screening_plan_id, district_id, vision_label_0_numbers, vision_label_1_numbers, vision_label_2_numbers, vision_label_3_numbers, key_warning_numbers, create_time
    </sql>

</mapper>
